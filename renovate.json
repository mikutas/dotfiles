{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":timezone(Asia/Tokyo)",
    "github>aquaproj/aqua-renovate-config#1.5.2",
    "github>aquaproj/aqua-renovate-config:installer-script#1.5.2(aliases\\..*sh)"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": ["aquaproj", "renovate/renovate"],
      "automerge": true,
      "extends": ["schedule:earlyMondays"]
    }
  ],
  "platformAutomerge": true,
  "recreateClosed": true,
  "regexManagers": [
    {
      "fileMatch": [
        "\\.config\\/fish\\/aliases\\..*sh"
      ],
      "matchStrings": [
        "renovate\\/renovate:(?<currentValue>.*)-slim"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "renovate/renovate"
    }
  ]
}
