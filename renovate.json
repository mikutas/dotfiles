{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>aquaproj/aqua-renovate-config#2.3.0",
    "github>aquaproj/aqua-renovate-config:installer-script#2.3.0(aliases\\..*sh)",
    "github>mikutas/my-renovate-config"
  ],
  "recreateWhen": "always",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "\\.config\\/fish\\/aliases\\..*sh"
      ],
      "matchStrings": [
        "renovate\\/renovate:(?<currentValue>.*)-slim"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "renovate/renovate"
    },
    {
      "customType": "regex",
      "fileMatch": [
        "test\\.yaml"
      ],
      "matchStrings": [
        "github\\.com\\/(?<depName>.*?)\\/blob\\/(?<currentValue>.*?)\\/deploy\\/charts\\/cert-manager\\/values.yaml"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "cert-manager/cert-manager"
    }
  ]
}
