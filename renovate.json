{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>aquaproj/aqua-renovate-config#1.2.6",
    "github>aquaproj/aqua-renovate-config:installer-script#1.2.6(aliases\\.fish)"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": ["aqua-registry", "renovate"],
      "automerge": true
    }
  ],
  "platformAutomerge": true,
  "regexManagers": [
    {
      "fileMatch": [
        ".config/fish/aliases.fish"
      ],
      "matchStrings": [
        "aqua-installer \\| bash -s -- -v (?<currentValue>.*)\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "aquaproj/aqua"
    },
    {
      "fileMatch": [
        ".config/fish/aliases.fish"
      ],
      "matchStrings": [
        "renovate\\/renovate:(?<currentValue>.*)-slim"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "renovate/renovate"
    }
  ]
}
