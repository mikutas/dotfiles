{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":timezone(Asia/Tokyo)",
    "github>aquaproj/aqua-renovate-config#1.3.0",
    "github>aquaproj/aqua-renovate-config:installer-script#1.3.0(aliases\\.fish)"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": ["aqua-registry", "renovate"],
      "automerge": true,
      "extends": ["schedule:earlyMondays"]
    }
  ],
  "platformAutomerge": true,
  "regexManagers": [
    {
      "fileMatch": [
        ".config/fish/aliases.fish"
      ],
      "matchStrings": [
        "renovate\\/renovate:(?<currentValue>.*)-slim"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "renovate/renovate"
    }
  ]
}
