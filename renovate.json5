{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>aquaproj/aqua-renovate-config#2.3.1",
    "github>aquaproj/aqua-renovate-config:installer-script#2.3.1(aliases\\..*sh)",
    "github>mikutas/my-renovate-config"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        // aquaだと
        // kubernetes/kubectl
        // asdfだと
        "kubectl"
      ],
      "allowedVersions": "<=1.28"
    }
  ],
  "recreateWhen": "always",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "aliases\\..*sh"
      ],
      "matchStrings": [
        "renovate\\/renovate:(?<currentValue>.*)-slim"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "renovate/renovate"
    }
  ]
}
