{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>aquaproj/aqua-renovate-config#2.4.0",
    "github>aquaproj/aqua-renovate-config:installer-script#2.4.0(aliases\\..*sh)",
    "github>mikutas/my-renovate-config"
  ],
  "packageRules": [
    {
      "matchDepNames": [
        "kubectl"
      ],
      "allowedVersions": "<=1.30"
    }
  ],
  "recreateWhen": "always",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "aliases\\..*sh"
      ],
      "matchStrings": [
        "renovate\\/renovate:(?<currentValue>.*) sh"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "renovate/renovate"
    },
    {
      "customType": "regex",
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "bitwarden/clients",
      "extractVersionTemplate": "^cli-(?<version>.*)$",
      "fileMatch": [
         "\\.?aqua\\.ya?ml"
      ],
      "matchStrings": [
         " +(?:version|'version'|\"version\") *: +cli-(?<currentValue>[^'\" \\n]+) +# renovate: depName=bitwarden/clients[ \\n]",
         " +(?:version|'version'|\"version\") *: +'cli-(?<currentValue>[^'\" \\n]+)' +# renovate: depName=bitwarden/clients[ \\n]",
         " +(?:version|'version'|\"version\") *: +\"cli-(?<currentValue>[^'\" \\n]+)\" +# renovate: depName=bitwarden/clients[ \\n]",
         " +(?:name|'name'|\"name\") *: +bitwarden/clients@cli-(?<currentValue>[^'\" \\n]+)",
         " +(?:name|'name'|\"name\") *: +'bitwarden/clients@cli-(?<currentValue>[^'\" \\n]+)'",
         " +(?:name|'name'|\"name\") *: +\"bitwarden/clients@cli-(?<currentValue>[^'\" \\n]+)\""
      ],
      "packageNameTemplate": "bitwarden/clients"
    }
  ]
}
